    "gulp-sass": "^1.3.3",
    "gulp-shell": "^0.4.0",
    "gulp-uglify": "^1.1.0",
    "mocha": "^2.0.1",
    "nodemon": "^1.1.1",
    "run-sequence": "^1.0.1",
    "tiny-lr": "^0.1.4",
    "uglify-js": "^2.4.14",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.3.0"
  },
  "engines": {
    "iojs": "1.5.x",
    "node": "0.10.x"
  }
}
