{
  "name": "epochtalk",
  "version": "0.3.0",
  "description": "Epochtalk forum software",
  "main": "server",
  "scripts": {
    "install": "./node_modules/.bin/napa",
    "db-create": "createdb $(cat database.json | ./node_modules/.bin/json ${NODE_ENV=development}.database)",
    "db-drop": "dropdb $(cat database.json | ./node_modules/.bin/json ${NODE_ENV=development}.database)",
    "db-migrate": "./node_modules/.bin/db-migrate -e ${NODE_ENV=development} -m ./node_modules/epochtalk-core-pg/migrations up",
    "db-recreate": "npm run db-drop && npm run db-create && npm run db-migrate",
    "db-devseed": "node ./tests/seed/devseed.js",
    "sass": "node ./scripts/sass.js",
    "build": "node ./scripts/build.js",
    "serve": "node ./scripts/serve.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/epochtalk/epochtalk"
  },
  "author": "James Wang <jw@slickage.com> (http://slickage.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slickage/epochtalk/issues"
  },
  "homepage": "https://github.com/slickage/epochtalk",
  "napa": {
    "angular": "angular/bower-angular#v1.3.14",
    "angular-resource": "angular/bower-angular-resource#v1.3.14",
    "angular-sanitize": "angular/bower-angular-sanitize#v1.3.14",
    "foundation": "zurb/bower-foundation",
    "nestable": "slickage/Nestable"
  },
  "dependencies": {
    "angular-ui-router": "^0.2.13",
    "async": "^0.9.0",
    "aws-sdk": "^2.1.20",
    "bcrypt": "^0.8.0",
    "bluebird": "^2.6.4",
    "boom": "^2.6.1",
    "brfs": "^1.3.0",
    "browserify": "^8.1.3",
    "cheerio": "^0.18.0",
    "commander": "^2.5.1",
    "db-migrate": "^0.8.0",
    "epochtalk-bbcode-parser": "^1.0.0",
    "epochtalk-core-pg": "^0.9.9",
    "epochtalk-emailer": "^0.0.2",
    "epochtalk-validator": "0.0.5",
    "fs-extra": "^0.16.5",
    "good": "^5.1.1",
    "good-console": "^4.1.0",
    "good-file": "^4.0.1",
    "hapi": "^8.2.0",
    "hapi-auth-jwt": "^2.0.0",
    "joi": "^6.0.8",
    "json": "^9.0.3",
    "jsonwebtoken": "1.3.x",
    "levelup": "^0.19.0",
    "lodash": "^2.4.1",
    "lout": "^6.1.0",
    "medium-editor": "^1.8.14",
    "memdown": "^0.11.0",
    "mkdirp": "^0.5.0",
    "mmmagic": "^0.3.11",
    "napa": "^1.0.1",
    "node-sass": "^3.0.0-alpha.0",
    "node-uuid": "^1.4.1",
    "pg": "^4.2.0",
    "request": "^2.53.0",
    "sanitize-html": "^1.4.3",
    "stream-meter": "^1.0.3",
    "through2": "^0.6.3",
    "yargs": "^1.2.1"
  },
  "devDependencies": {
    "del": "^1.1.1",
    "livereload": "^0.3.6",
    "mocha": "^2.0.1",
    "nodemon": "^1.1.1",
    "watchify": "^2.4.0"
  },
  "engines": {
    "iojs": "1.5.x",
    "node": "0.10.x"
  }
}
