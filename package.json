{
  "name": "epochtalk",
  "version": "0.3.0",
  "description": "Epochtalk forum software",
  "main": "server",
  "scripts": {
    "db-create": "createdb $(cat database.json | ./node_modules/.bin/json ${NODE_ENV=development}.database)",
    "db-drop": "dropdb $(cat database.json | ./node_modules/.bin/json ${NODE_ENV=development}.database)",
    "db-migrate": "./node_modules/.bin/db-migrate -e ${NODE_ENV=development} -m ./node_modules/epochtalk-core-pg/migrations up",
    "db-recreate": "npm run db-drop && npm run db-create && npm run db-migrate",
    "db-devseed": "node ./tests/seed/devseed.js",
    "install": "./node_modules/.bin/napa",
    "build": "./node_modules/.bin/gulp build",
    "dev-fe": "node server & ./node_modules/.bin/gulp default",
    "dev-be": "nodemon server",
    "start": "foreman start",
    "clean": "./node_modules/.bin/gulp clean",
    "test": "./node_modules/.bin/mocha -R spec ./tests"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/epochtalk/epochtalk"
  },
  "author": "James Wang <jw@slickage.com> (http://slickage.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slickage/epochtalk/issues"
  },
  "homepage": "https://github.com/slickage/epochtalk",
  "napa": {
    "angular": "angular/bower-angular#v1.3.1-build.3464+sha.d97b427",
    "angular-cookies": "angular/bower-angular-cookies#v1.3.1-build.3464+sha.d97b427",
    "angular-resource": "angular/bower-angular-resource#v1.3.1-build.3464+sha.d97b427",
    "angular-route": "angular/bower-angular-route#v1.3.1-build.3464+sha.d97b427",
    "angular-sanitize": "angular/bower-angular-sanitize#v1.3.1-build.3464+sha.d97b427",
    "foundation": "zurb/bower-foundation",
    "nestable": "slickage/Nestable"
  },
  "dependencies": {
    "angular-ui-router": "^0.2.11",
    "async": "^0.9.0",
    "bcrypt": "^0.8.0",
    "bluebird": "^2.6.4",
    "boom": "^2.6.1",
    "brfs": "^1.3.0",
    "browserify": "^8.1.3",
    "cheerio": "^0.18.0",
    "commander": "^2.5.1",
    "db-migrate": "^0.8.0",
    "epochtalk-bbcode-parser": "^1.0.0",
    "epochtalk-core-pg": "^0.9.7",
    "epochtalk-emailer": "^0.0.1",
    "epochtalk-image-server": "^0.1.0",
    "epochtalk-validator": "0.0.3",
    "good": "^5.1.1",
    "good-console": "^4.1.0",
    "good-file": "^4.0.1",
    "hapi": "^8.2.0",
    "hapi-auth-jwt": "^2.0.0",
    "joi": "^6.0.8",
    "json": "^9.0.3",
    "jsonwebtoken": "1.3.x",
    "levelup": "^0.19.0",
    "lodash": "^2.4.1",
    "lout": "^6.1.0",
    "medium-editor": "^1.8.14",
    "memdown": "^0.11.0",
    "mkdirp": "^0.5.0",
    "mmmagic": "^0.3.11",
    "napa": "^1.0.1",
    "node-uuid": "^1.4.1",
    "pg": "^4.2.0",
    "pkgcloud": "^1.0.3",
    "request": "^2.53.0",
